{
    "app":{
        "cfengine":"lucee@6",
        "serverHomeDirectory":".engines/lucee6"
    },
    "name":"contentbox-lucee6",
    "JVM":{
        "javaVersion":"openjdk21_jre",
        "heapSize":"1024",
        "args":"-Dcom.sun.net.ssl.enableECC=false"
    },
    "web":{
        "directoryBrowsing":true,
        "rewrites":{
            "enable":true
        },
        "http":{
            "port":"8589"
        }
    },
    "trayicon":"modules_app/contentbox-custom/_content/ContentBoxIcon300.png",
    "trayOptions":[
        {
            "label":"Open ContentBox Admin",
            "action":"openbrowser",
            "url":"/cbadmin",
            "disabled":false,
            "image":"modules_app/contentbox-custom/_content/ContentBoxIcon300.png"
        }
    ],
    "cfconfig":{
        "file":".cfconfig.json"
    },
    "env":{
        "lucee-extensions":"D062D72F-F8A2-46F0-8CBC91325B2F067B"
    }
}
